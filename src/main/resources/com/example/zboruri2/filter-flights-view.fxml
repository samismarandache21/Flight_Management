<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.zboruri2.controller.FilterFlightsController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox fx:id="filterBox">
        <DatePicker fx:id="datePicker"/>
        <ComboBox fx:id="fromBox" promptText="FROM"/>
        <ComboBox fx:id="toBox" promptText="TO"/>
        <Button fx:id="filterButton" text="FILTER" onAction="#filterClick"/>
        <Button fx:id="buyTicketButton" text="BUY TICKET" onAction="#buyClick"/>
    </VBox>

</AnchorPane>
